<h1>Welcome Matt Bicycle Shop</h1>
<%= form_for @user do |u| %>
<%= render partial: "layouts/errors", locals: {record: @user} %>
<%= u.label :username %>
<%= u.text_field :username%><br>
<%= u.label :email %>
<%= u.text_field :email%><br>
<%= u.label :password %>
<%= u.password_field :password%><br>
<%= u.label :password_confirmation %>
<%= u.password_field :password_confirmation%><br>
<%= u.submit "Sign Up!"%><br>

<% end %>
